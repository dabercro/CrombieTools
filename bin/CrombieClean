#! /bin/bash

object=$1

find $CROMBIEPATH/*Tools -name $object*.so  | xargs rm
find $CROMBIEPATH/*Tools -name $object*.d   | xargs rm
find $CROMBIEPATH/*Tools -name $object*.pcm | xargs rm
find $CROMBIEPATH/python -name $object*.pyc | xargs rm

pycacheDir=$CROMBIEPATH/python/CrombieTools/__pycache__
if [ -d $pycacheDir ]
then
    rmdir $pycacheDir
fi
