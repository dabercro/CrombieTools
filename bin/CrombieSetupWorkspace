#! /bin/bash

ConfigName=CrombieSlimmingConfig.sh

if [ ! -f $ConfigName ]
then
    cp $CROMBIEPATH/templates/$ConfigName .
fi

SlimmerName=runSlimmer.py

if [ ! -f $SlimmerName ]
then
    cp $CROMBIEPATH/templates/$SlimmerName .
fi

ScriptList=CrombieJobScriptList.txt

if [ ! -f $ScriptList ]
then
    cp $CROMBIEPATH/templates/$ScriptList .
fi

FlatSkimmer=FlatSkimmer.sh

if [ ! -f $FlatSkimmer ]
then
    cp $CROMBIEPATH/templates/$FlatSkimmer .
fi
